  "peerDependencies": {
    "immutable": ">=3.6.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.9.1",
    "expect.js": "^0.3.1",
    "immutable": "4.0.0-rc.9",
    "mocha": "^5.2.0",
    "prop-types": "15.6.1",
